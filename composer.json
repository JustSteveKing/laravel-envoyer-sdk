{
  "name": "juststeveking/laravel-envoyer-sdk",
  "description": "A simple to use PHP class to work with the Laravel Envoyer API",
  "keywords": [
    "laravel",
    "envoyer",
    "sdk"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "role": "developer",
      "name": "Steve McDougall",
      "email": "juststevemcd@gmail.com",
      "homepage": "https://www.juststeveking.uk/"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "juststeveking/http-auth-strategies": "^1.1",
    "juststeveking/http-slim": "^1.2",
    "juststeveking/php-sdk": "^1.1",
    "juststeveking/uri-builder": "^1.1",
    "nyholm/psr7": "^1.3",
    "php-di/php-di": "^6.3",
    "symfony/http-client": "^5.1"
  },
  "require-dev": {
    "php-http/mock-client": "^1.4",
    "phpunit/phpunit": "^9.4",
    "symfony/var-dumper": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "JustSteveKing\\Laravel\\Envoyer\\SDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JustSteveKing\\Laravel\\Envoyer\\SDK\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
